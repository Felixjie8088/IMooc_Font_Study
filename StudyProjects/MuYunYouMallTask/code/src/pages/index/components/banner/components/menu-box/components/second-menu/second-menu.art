<div class="menu" data-n="{{key}}">
    <div class="menu_inn">
        <dl>
            {{each items}}
            <dt>{{$value.title}}</dt>
            <dd>
                {{each $value.cities item}}
                <a href="{{item.link}}">{{item.text}}</a>
                {{/each}}
            </dd>
            {{/each}}
        </dl>
    </div>
</div>