<div class="hd">
    <h3>{{title}}</h3>
    <div class="slogan">{{slogan}}</div>
    <div class={{isDisplay?"tabbar":"dpn"}}>
        <ul>
            {{each items}}
            <li class={{$value.curr?"current":""}}><a href="javascript:">{{$value.text}}</a></li>
            {{/each}}
        </ul>
    </div>
</div>