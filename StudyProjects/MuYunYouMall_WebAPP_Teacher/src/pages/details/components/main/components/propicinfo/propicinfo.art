<div id="product-pic" class="product-pic"></div>
<div class="product-info center">
    <p class="pro-text">{{title}}</p>
    <p class="pro-freebie">
        {{each tags val}}
        <a href="javascript:">{{val}}</a>
        {{/each}}
    </p>
    <div class="pro-ll-ys">
        <span class="ll">浏览{{vistor_counts}}</span>
        <span class="ys">已售{{sold_counts}}份</span>
    </div>
</div>